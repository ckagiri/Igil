@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en" class="fuelux">
    <head>
            <title>Ligi Admin</title>

        <meta charset="utf-8" />

        <!-- Mobile viewport optimized: h5bp.com/viewport -->
        <meta name="viewport" content="width=device-width">
    @Styles.Render("~/Content/css")
    <script>
        // Must be first. IE10 mobile viewport fix
        if (navigator.userAgent.match(/IEMobile\/10\.0/)) {
            var msViewportStyle = document.createElement("style");
            var mq = "@@-ms-viewport{width:auto!important}";
            msViewportStyle.appendChild(document.createTextNode(mq));
            document.getElementsByTagName("head")[0].appendChild(msViewportStyle);
        }
    </script>
        <!-- All JavaScript at the bottom, except this Modernizr build.
            Modernizr enables HTML5 elements & feature detects for optimal performance.
            Create your own custom Modernizr build: www.modernizr.com/download/ -->
        @Scripts.Render("~/bundles/modernizr")
    </head>
<body>
    <div class="navbar  navbar-fixed-top navbar-inverse">
        <div class="navbar-inner">
            <div class="container">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <a class="brand" href="/">Ligi Admin</a>
                <div id="shellTop-view" class="nav-collapse" >
                    <ul class="nav" data-bind="with: menuHashes">
                        <li><a data-bind="attr: { href: fixtures }">Fixtures</a></li>
                        <li><a data-bind="attr: { href: results }">Results</a></li>
                        <li><a data-bind="attr: { href: odds }">Odds</a></li>
                        <li><a data-bind="attr: { href: teams }">Teams</a></li>
                        <li><a data-bind="attr: { href: leagues }">Leagues</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <section class="main">
        @RenderPage("Views/leagues.cshtml")
        @RenderPage("Views/teams.cshtml")
        @RenderPage("Views/team.cshtml")
        @RenderPage("Views/teamadd.cshtml")
        @RenderPage("Views/fixtures.cshtml")
        @RenderPage("Views/fixture.cshtml")
        @RenderPage("Views/fixtureadd.cshtml")
        @RenderPage("Views/odds.cshtml")
        @RenderPage("Views/matchodds.cshtml")
        @RenderPage("Views/results.cshtml")
        @RenderPage("Views/result.cshtml")
    </section>
    @Scripts.Render(
        "~/bundles/jquery", 
        "~/Content/js",
        "~/bundles/jsextlibs", 
        "~/Scripts/lib/require.js", 
        "~/bundles/jsapplibs",
        "~/Scripts/main.js"
    )
    
    <script src="Content/js/bootstrap.min.js"></script>
    <script src="Content/js/bootstrap-datetimepicker.js"></script>

</body>
</html>




