<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Ligi</title>
    <link href="~/Assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Assets/css/jquery-ui-1.10.0.custom.css" rel="stylesheet" />
    <link href="~/Assets2/css/apps/_base.css" rel="stylesheet" />
    <link href="~/Assets2/css/apps/footer.css" rel="stylesheet" />
    <link href="~/Assets2/css/apps/header.css" rel="stylesheet" />
    <link href="~/Assets2/css/apps/spinner.css" rel="stylesheet" />
</head>
    <body>
        <div id="wrap">
            <div id="header-region"></div>
            <div id="main-region" class="container"></div>
        </div>
        <div id="footer-region"></div>
        
      
        <script type="text/template" id="header-tmpl">
            <div id="header" class="navbar">
                <div class="navbar-inner">
                    <div class="container">
                        <div class="span12">
                            <div class="row">
                                <div class="pull-left">
                                    <span class="brand">Loading Views</span>
                                </div>
                                <ul class="nav pull-right">
                                    <li>
                                        <a href="#dashboard">
                                            <i class="icon-home"></i>
                                            Dashboard
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#search">
                                            <i class="icon-search"></i>
                                            Search
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#rentals">
                                            <i class="icon-list-alt"></i>
                                            Rentals
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </script>
        
        <script type="text/template" id="push">
            
        </script>

        <script type="text/template" id="show-layout-tmpl">
            <div class="row">
                <div class="span6">
                    <section class="lead">
                        <h3>Upcoming Movies
                        </h3>
                    </section>
                    <div id="upcoming-region"></div>
                </div>
                <div class="span6">
                    <section class="lead">
                        <h3>In Theatres
                        </h3>
                    </section>
                    <div id="theatre-region"></div>
                </div>
            </div>
        </script>
        
        <script type="text/template" id="theatre-tmpl">
            <td>
                 <% if (typeof posters != 'undefined' && posters || undefined) { %>
		            <div class="thumbnail">
                        <img src="<%= posters.thumbnail %>" />
                    </div>
                <% } %>
            </td>
            <td><%= title %></td>
            <td><%= mpaa_rating %></td>
            <td><%= ratings.critics_score %></td>
        </script>
        
        <script type="text/template" id="theatres-tmpl">
            <table class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th></th>
                        <th>Title</th>
                        <th>Rating</th>
                        <th>Score</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </script>
        
        <script type="text/template" id="upcoming-movie-tmpl">
            <td>
                 <% if (typeof posters != 'undefined' && posters || undefined) { %>
                    <div class="thumbnail">
                    <img src="<%= posters.thumbnail %>" />
                    </div>
                <% } %>
            </td>
            <td><%= title %></td>
            <td><%= mpaa_rating %></td>
            <td><% if(release_dates) {  %>
                <span><%= release_dates.theater %></span>
                <% } %>
            </td>
        </script>
        
        <script type="text/template" id="upcoming-movies-tmpl">
            <table class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th></th>
                        <th>Title</th>
                        <th>Rating</th>
                        <th>Date</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </script>
        
        <script type="text/template" id="hero-tmpl">
            <div class="hero-unit">
                <h2>Go on! Get searching...</h2>
                <p>Type in the search input above to find movie results.</p>
            </div>
        </script>
        
        <script type="text/template" id="movie-tmpl">
            <td>
                 <% if (typeof posters != 'undefined' && posters || undefined) { %>
		        <div class="thumbnail">
                    <img src="<%= posters.thumbnail %>" />
                </div>
                <% } %>
            </td>
            <td><%= title %></td>
            <td><%= year %></td>
            <td><%= mpaa_rating %></td>
            <td><%= runtime %> minutes</td>
            <td><%= ratings.critics_score %></td>
        </script>
        
        <script type="text/template" id="movie-empty-tmpl">
            <td colspan="7">
                <div class="well text-center">
                    Sorry, no movies found!
                </div>
            </td>
        </script>
        
        <script type="text/template" id="movies-tmpl">
            <table class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th></th>
                        <th>Title</th>
                        <th>Released</th>
                        <th>Rating</th>
                        <th>Runtime</th>
                        <th>Score</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </script>

        <script type="text/template" id="panel-tmpl">
            <section class="lead">
                <h3 class="pull-left">Search Movies</h3>
                <form class="form-search pull-right">
                    <div class="input-append">
                        <input type="text" class="span4 search-query" placeholder="Search for a movie...">
                        <button type="submit" class="btn">Search</button>
                    </div>
                </form>
            </section>
        </script>
        
        <script type="text/template" id="movies-layout-tmpl">
            <div class="span12">
                <div class="row">
                    <div id="panel-region"></div>
                    <div id="movies-region"></div>
                </div>
            </div>
        </script>
        
        <script type="text/template" id="pagination-tmpl">
            <div class="pagination pull-right">
                <ul>
                    <li class="disabled">
                        <a href="#">&laquo;</a>
                    </li>
                    <li class="active">
                        <span>1</span>
                    </li>
                    <li class="disabled">
                        <a href="#">&raquo;</a>
                    </li>
                </ul>
            </div>
        </script>

        <script type="text/template" id="rental-tmpl">
            <td>
                <% if (typeof posters != 'undefined' && posters || undefined) { %>
		        <div class="thumbnail">
                    <img src="<%= posters.thumbnail %>" />
                </div>
                <% } %>
            </td>
            <td><%= title %></td>
            <td><%= year %></td>
            <td><%= mpaa_rating %></td>
            <td><%= runtime %> minutes</td>
            <td><%= ratings.critics_score %></td>
        </script>
        
        <script type="text/template" id="rentals-tmpl">
            <table class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th></th>
                        <th>Title</th>
                        <th>Released</th>
                        <th>Rating</th>
                        <th>Runtime</th>
                        <th>Score</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </script>
        
        <script type="text/template" id="results-tmpl">
            <section class="lead">
                <h3>
                    Top 'US' Rentals
                    <small class="pull-right">
                        Found <span class="badge badge-info"><%= items.length %></span> results
                    </small>
                </h3>
            </section>
        </script>
        
        <script type="text/template" id="rentals-layout-tmpl">
            <div class="span12">
                <div class="row">
                    <div id="results-region"></div>
                    <div id="rentals-region"></div>
                    <div id="pagination-region"></div>
                </div>
            </div>
        </script>
   
        <script src="~/Assets/js/vendor/jquery.js"></script>
        <script src="~/Assets/js/vendor/jquery-ui-1.10.3.js"></script>
        <script src="~/Assets/js/vendor/json2.js"></script>
        <script src="~/Assets/js/vendor/underscore.js"></script>
        <script src="~/Assets/js/vendor/backbone.js"></script>
        <script src="~/Assets/js/vendor/backbone.picky.js"></script>
        <script src="~/Assets/js/vendor/backbone.syphon.js"></script>
        <script src="~/Assets/js/vendor/backbone.localstorage.js"></script>
        <script src="~/Assets/js/vendor/backbone.marionette.js"></script>
        <script src="~/Assets/js/vendor/spin.js"></script>
        <script src="~/Assets/js/vendor/spin.jquery.js"></script>
        <script src="~/Assets2/js/ligiadmin/config/jquery.js"></script>
        <script src="~/Assets2/js/ligiadmin/config/backbone/sync.js"></script>
        <script src="~/Assets2/js/ligiadmin/app2.js"></script>
@*        <script src="~/Assets2/js/ligiadmin/config/marionette/renderer.js"></script>*@
        <script src="~/Assets2/js/ligiadmin/config/marionette/application.js"></script>
        <script src="~/Assets2/js/ligiadmin/config/javascript.js"></script>
        <script src="~/Assets2/js/ligiadmin/entities/_base/model.js"></script>
        <script src="~/Assets2/js/ligiadmin/entities/_base/collections.js"></script>
        <script src="~/Assets2/js/ligiadmin/entities/_base/_fetch.js"></script>
        <script src="~/Assets2/js/ligiadmin/entities/abstract/buttons.js"></script>
        <script src="~/Assets2/js/ligiadmin/entities/movies.js"></script>
        <script src="~/Assets2/js/ligiadmin/controllers/_base.js"></script>
        <script src="~/Assets2/js/ligiadmin/views/_base/view.js"></script>
        <script src="~/Assets2/js/ligiadmin/views/_base/collectionview.js"></script>
        <script src="~/Assets2/js/ligiadmin/views/_base/compositeview.js"></script>
        <script src="~/Assets2/js/ligiadmin/views/_base/itemview.js"></script>
        <script src="~/Assets2/js/ligiadmin/views/_base/layout.js"></script>
        <script src="~/Assets2/js/ligiadmin/components/form/form_controller.js"></script>
        <script src="~/Assets2/js/ligiadmin/components/form/form_view.js"></script>
        <script src="~/Assets2/js/ligiadmin/components/loading/loading_controller.js"></script>
        <script src="~/Assets2/js/ligiadmin/components/loading/loading_view.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/header2/header_app.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/header2/list/list_controller.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/header2/list/list_view.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/dashboard/dashboard_app.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/dashboard/show/show_controller.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/dashboard/show/show_view.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/dashboard_theatres/dashboard_theatres_app.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/dashboard_theatres/list/list_controller.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/dashboard_theatres/list/list_view.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/dashboard_upcoming/dashboard_upcoming_app.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/dashboard_upcoming/list/list_controller.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/dashboard_upcoming/list/list_view.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/rentals/rentals_app.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/rentals/list/list_controller.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/rentals/list/list_view.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/search/search_app.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/search/list/list_controller.js"></script>
        <script src="~/Assets2/js/ligiadmin/apps/search/list/list_view.js"></script>
        <script type="text/javascript">
            LigiAdmin.start();
        </script>
    </body>
</html>
